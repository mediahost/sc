facebook:
	appId: "app_id"
	appSecret: "app_secret"

twitter:
	consumerKey: 'consumer_key'
	consumerSecretKey: 'secret_key'

parameters:
	install:
		lockInstaller: TRUE # při TRUE zamyká instalaci po spuštění, 
							# pak je potřeba odmazat temp/install pro novou instalaci
